{
  "name": "@esmkit/diff-objects",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Deep diffs two objects, including nested structures of arrays and objects, and return the difference.",
  "author": "Billgo <hi@billgo.me>",
  "bugs": "https://github.com/esmkit/diff-objects/issues",
  "repository": "git+ssh://git@github.com:esmkit/diff-objects.git",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./*": "./*"
  },
  "scripts": {
    "prepublishOnly": "bun run build",
    "build": "bunx rimraf dist && bun run build:esm && bun run build:bun",
    "build:bun": "bun run bun.build.mjs",
    "build:esm": "bunx tsup",
    "start": "bun run --watch examples/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.4",
    "@devjskit/biome-config": "^0.2.0",
    "@types/bun": "^1.2.17",
    "@types/node": "^24.0.3",
    "@types/web": "^0.0.243",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
